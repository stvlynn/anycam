# API Key for Nano Banana (Gemini 3 Pro Image Preview)
NANO_BANANA_API_KEY=your_api_key_here

# API Base URL (images edits)
NANO_BANANA_API_URL=https://api.tu-zi.com/v1/images/edits

# Model Name
NANO_BANANA_MODEL=gemini-3-pro-image-preview

# Response format: url | b64_json
NANO_BANANA_RESPONSE_FORMAT=url

# Optional quality: 1k | 2k | 4k (banana 2 only)
NANO_BANANA_QUALITY=

# Optional size: {w}x{h}, e.g., 1024x1024; leave blank to use service default
NANO_BANANA_SIZE=

# Request timeout in ms
TIMEOUT_MS=60000

# Whether to send a transparent mask (edit full image). true|false
NANO_BANANA_USE_MASK=false

# Google Cloud API key for Street View
GOOGLE_CLOUD_API_KEY=

# Server Port
PORT=3000
